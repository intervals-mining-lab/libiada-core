<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class name="ChainAnalises.Classes.AuxiliaryClasses.DBInterface.DBFile, ChainAnalises" table="node" lazy="false">
    <id name="nid">
      <column name="nid" sql-type="int(10)" not-null="true"/>
      <generator class="native" />
    </id>

    <property name="type" column="type"/>
    <property name="title" column="title"/>
    <property name="uid" column="uid"/>
    <property name="status" column="status"/>
    <property name="language" column="language"/>
    <property name="comment" column="comment"/>
    <property name="moderate" column="moderate"/>
    <property name="use_trigger" column="use_trigger"/>    
    
    <join table="content_type_file" >
      <key>
        <column name = "nid"/>
      </key>      
      <property name="value" type="string" column="field_file_value"/>
      <many-to-one name="field_type" class="ChainAnalises.Classes.AuxiliaryClasses.DBInterface.DBFileType, ChainAnalises" column="field_reference_file_type_nid"/>
    </join>    

  </class>  
</hibernate-mapping>
