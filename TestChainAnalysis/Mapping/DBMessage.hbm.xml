<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class name="ChainAnalises.Classes.AuxiliaryClasses.DBInterface.DBMessage, ChainAnalises" table="node" lazy="false">
    <composite-id>
      <key-property name="type" column="type"/>
      <key-property name="hash_md5" column="hash"/>
    </composite-id>

    <property name="nid" column="nid"/>
    <property name="title" column="title"/>
    <property name="uid" column="uid"/>
    <property name="status" column="status"/>
    <property name="language" column="language"/>
    <property name="comment" column="comment"/>
    <property name="moderate" column="moderate"/>
    <property name="use_trigger" column="use_trigger"/>   
    
    <join table="content_type_element" >
      <key>
        <column name = "nid"/>
      </key>      
      <property name="value" type="string" column="field_value_value"/>
    </join>
    


<!--    <join table="content_field_hash_md5" >
      <key>
        <column name = "nid"/>
      </key>
    </join> -->
  </class>  
</hibernate-mapping>
