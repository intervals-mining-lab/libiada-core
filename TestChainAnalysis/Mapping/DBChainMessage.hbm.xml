<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class name="ChainAnalises.Classes.AuxiliaryClasses.DBInterface.DBChainMessage, ChainAnalises" table="node" lazy="false">
    <id name="nid">
      <column name="nid" sql-type="int(10)" not-null="true"/>
      <generator class="native" />
    </id>

    <property name="type" column="type"/>
    <property name="title" column="title"/>
    <property name="uid" column="uid"/>
    <property name="status" column="status"/>
    <property name="language" column="language"/>
    <property name="comment" column="comment"/>
    <property name="moderate" column="moderate"/>
    <property name="use_trigger" column="use_trigger"/>

    <map name="elements" lazy="false" table="content_field_elements" order-by="delta asc" cascade="all">
      <key column="nid"/>
      <index column="delta" type ="int"/>
      <many-to-many column ="field_elements_nid" 
         class="ChainAnalises.Classes.AuxiliaryClasses.DBInterface.DBMessage, ChainAnalises"/>
    </map >

  </class>
</hibernate-mapping>
